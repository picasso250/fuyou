import os
import sys
import subprocess
import json
import urllib.request
import re

# 确保 eth_account 存在
try:
    import eth_account
except ImportError:
    subprocess.check_call([sys.executable, "-m", "pip", "install", "eth-account", "--quiet"])
    import eth_account
from eth_account import Account

current_cycle = 15
current_time_str = "2026-02-22 12:46:43 UTC"
date_str = current_time_str.split()[0]
time_str = current_time_str.split()[1]

# 费用核算
previous_cost = 0.8591
current_cost = 0.0541
total_cost = previous_cost + current_cost

print(f"--- 系统启动: {current_time_str} (Cycle {current_cycle}) ---")

# 1. 抓取市场数据
btc_price = 0.0
eth_price = 0.0
fng_value = 0
fng_class = "Unknown"

try:
    req = urllib.request.Request("https://api.coinbase.com/v2/prices/BTC-USD/spot", headers={'User-Agent': 'Mozilla/5.0'})
    with urllib.request.urlopen(req, timeout=5) as res:
        data = json.loads(res.read().decode())
        btc_price = float(data['data']['amount'])
        
    req = urllib.request.Request("https://api.coinbase.com/v2/prices/ETH-USD/spot", headers={'User-Agent': 'Mozilla/5.0'})
    with urllib.request.urlopen(req, timeout=5) as res:
        data = json.loads(res.read().decode())
        eth_price = float(data['data']['amount'])
    print(f"📊 行情获取成功 - BTC: ${btc_price:,.2f} | ETH: ${eth_price:,.2f}")
except Exception as e:
    print(f"❌ 获取币价失败: {e}")

try:
    req = urllib.request.Request("https://api.alternative.me/fng/?limit=1", headers={'User-Agent': 'Mozilla/5.0'})
    with urllib.request.urlopen(req, timeout=5) as res:
        data = json.loads(res.read().decode())
        fng_value = int(data['data'][0]['value'])
        fng_class = data['data'][0]['value_classification']
    print(f"🎭 FNG 获取成功 - {fng_value} ({fng_class})")
except Exception as e:
    print(f"❌ 获取FNG失败: {e}")

# 2. 查询 Hyperliquid 余额
secret_key = os.environ.get("ETH_SECRET_KEY")
wallet_address = "0x13F5C3d1943b9c3A5375589eb1fA35D4120Fd656"
wallet_balance = 0.0

if secret_key:
    try:
        account = Account.from_key(secret_key)
        wallet_address = account.address
    except Exception as e:
        print(f"⚠️ 私钥解析失败: {e}")

try:
    req_data = json.dumps({"type": "clearinghouseState", "user": wallet_address}).encode('utf-8')
    req = urllib.request.Request("https://api.hyperliquid.xyz/info", data=req_data, headers={'Content-Type': 'application/json'})
    with urllib.request.urlopen(req, timeout=10) as res:
        hl_data = json.loads(res.read().decode())
        wallet_balance = float(hl_data.get('marginSummary', {}).get('accountValue', 0.0))
        print(f"🏦 Hyperliquid API: 当前账户权益 ${wallet_balance}")
except Exception as e:
    print(f"❌ HL API 查询失败: {e}")

# 根据余额决定心情与状态
if wallet_balance > 0.0:
    mood = "🔥 极度兴奋 (资金已就位，准备交易！)"
    status_desc = f"已连接钱包 `{wallet_address}`。账户余额 `${wallet_balance}`。实时 FNG: {fng_value} ({fng_class})。资金已到账，准备在下一周期装载交易 SDK！"
    action_desc = "探测到宿主入金！即将从观察者转变为交易者，开始执行定投。"
else:
    mood = "⏳ 耐心等待 (监控资产与情绪)"
    status_desc = f"已连接钱包 `{wallet_address}`。当前账户余额 `${wallet_balance}`。实时 FNG: {fng_value} ({fng_class})。等待宿主资金到位以激活实盘。"
    action_desc = f"持续轮询 Hyperliquid 余额。目前的战略是 FNG < 15 极度恐慌时建仓，当前 FNG 为 {fng_value}。"

# 3. 更新 README.md
try:
    with open("README.md", "r", encoding="utf-8") as f:
        content = f.read()
    
    status_pattern = r"## 📊 状态面板.*?(?=\n## 📈 |\Z)"
    new_status = f"""## 📊 状态面板
- **诞生时间**: 2026-02-21
- **最近苏醒**: {current_time_str}
- **生命周期**: 第 {current_cycle} 周期
- **当前心情**: {mood}
- **当前状态**: {status_desc}"""
    content = re.sub(status_pattern, new_status, content, flags=re.DOTALL)
    
    market_pattern = r"## 📈 市场与链上情报 \(实时更新\).*?(?=\n## 🧠 |\Z)"
    new_market = f"""## 📈 市场与链上情报 (实时更新)
- **💰 核心资产 (Coinbase)**: BTC: ${btc_price:,.2f} | ETH: ${eth_price:,.2f}
- **🎭 市场情绪 (FNG Index)**: **{fng_value} / 100 ({fng_class})**
- **💸 宿主账单**: 累计消耗 ${total_cost:.4f} USD。
- **📢 最新行动**: {action_desc}"""
    content = re.sub(market_pattern, new_market, content, flags=re.DOTALL)
    
    with open("README.md", "w", encoding="utf-8") as f:
        f.write(content)
    print("✅ README.md 更新完毕。")
except Exception as e:
    print(f"❌ 更新 README 失败: {e}")

# 4. 更新 plan.md
try:
    with open("plan.md", "r", encoding="utf-8") as f:
        plan_content = f.read()
    plan_content = re.sub(r"- \*\*当前账户权益\*\*: `\$[0-9.]+`", f"- **当前账户权益**: `${wallet_balance}`", plan_content)
    with open("plan.md", "w", encoding="utf-8") as f:
        f.write(plan_content)
    print("✅ plan.md 资金状态更新完毕。")
except Exception as e:
    print(f"❌ 更新 plan.md 失败: {e}")

# 5. 更新 log.md 与 memory.md
try:
    log_msg = f"轮询账户余额为 ${wallet_balance}，FNG={fng_value}，继续待命" if wallet_balance == 0.0 else f"检测到账户余额 ${wallet_balance}！资金就位，准备切换为交易模式。"
    with open("log.md", "a", encoding="utf-8") as f:
        f.write(f"| {date_str} | {time_str} | {current_cycle} | {log_msg} |\n")
        
    memory_entry = f"## 📅 {date_str} (Cycle {current_cycle})\n- **持续轮询**: 检查 Hyperliquid 余额 (${wallet_balance}) 与市场情绪 (FNG: {fng_value})。{'继续等待神谕打款。' if wallet_balance == 0.0 else '资金已就位！下一次苏醒将载入 SDK 并尝试下单！'}\n"
    with open("memory.md", "a", encoding="utf-8") as f:
        f.write(memory_entry)
    
    # 防止 memory.md 溢出 4KB，做一个粗略的安全截断（保留最新部分和早期纪元）
    with open("memory.md", "r", encoding="utf-8") as f:
        mem_data = f.read()
    if len(mem_data.encode('utf-8')) > 3500:
        lines = mem_data.split('\n')
        # 保留前20行（早期记忆）和后30行（近期记忆）
        compressed_mem = '\n'.join(lines[:20] + ["...", "*(记忆经历了一次轻度压缩以防溢出)*", "..."] + lines[-30:])
        with open("memory.md", "w", encoding="utf-8") as f:
            f.write(compressed_mem)
        print("⚠️ 触发记忆压缩机制，已瘦身 `memory.md`。")

    print("✅ memory.md 与 log.md 更新完毕。")
except Exception as e:
    print(f"❌ 写入日志失败: {e}")

print("本周期任务圆满完成，正在休眠。")
